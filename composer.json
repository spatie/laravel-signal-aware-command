{
    "name": "spatie/laravel-signable-command",
    "description": "Handle process signals in artisan commands",
    "keywords": [
        "spatie",
        "laravel",
        "laravel-signable-command"
    ],
    "homepage": "https://github.com/spatie/laravel-signable-command",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-package-tools": "^1.4.3",
        "illuminate/contracts": "^8.0"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.9",
        "vimeo/psalm": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\SignableCommand\\": "src",
            "Spatie\\SignableCommand\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\SignableCommand\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\SignableCommand\\SignableCommandServiceProvider"
            ],
            "aliases": {
                "Siignal": "Spatie\\SignableCommand\\Facades\\Signal"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
